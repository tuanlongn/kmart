{
  "name": "kmart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'prisma generate --watch' 'yarn codegen:watch' 'next dev'",
    "build": "prisma generate && graphql-codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch './src/**/*.{gql,graphql}'"
  },
  "prisma": {
    "schema": "src/db/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@graphql-yoga/node": "^2.9.1",
    "@pothos/core": "^3.11.1",
    "@pothos/plugin-prisma": "^3.9.0",
    "@pothos/plugin-scope-auth": "^3.9.1",
    "@prisma/client": "^3.14.0",
    "ag-grid-community": "^27.3.0",
    "ag-grid-enterprise": "^27.3.0",
    "ag-grid-react": "^27.3.0",
    "autoprefixer": "^10.4.7",
    "cuid": "^2.1.8",
    "graphql": "^16.5.0",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "postcss": "^8.4.14",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "tailwindcss": "^3.1.1"
  },
  "devDependencies": {
    "@boost/module": "^3.1.0",
    "@faker-js/faker": "^7.2.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "concurrently": "^7.2.2",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3"
  }
}